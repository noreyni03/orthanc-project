{
    "compilerOptions": {
      "sourceMap": true,
      "outDir": "dist",
      "strict": true,
      "lib": ["esnext"],
      "esModuleInterop": true,
      "module": "CommonJS",
      "moduleResolution": "node",
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "isolatedModules": true
    },
    "include": ["./seed.ts"],
    "exclude": ["node_modules"]
